import numpy as np 
from optproblems import cec2005

n_dimensions = 2
pop_size = 10

function = cec2005.F2(n_dimensions)
ideal = function(function.offsets[0:n_dimensions])

adult_p = [] #Empty adult population
adult_fitness = [] #Corresponding fitness list 


#Initializing random adult population and fitness  vectors,
for i in range(pop_size):
    #creating population 
    adult_p.append(np.random.uniform(function.min_bounds, function.max_bounds,n_dimensions)) 
    #Populating fitness value for each member of population in list
    adult_fitness.append(function(adult_p[i])-ideal)

best = np.argmin(adult_fitness)

print(adult_p[best]) #fittest member of population location vector